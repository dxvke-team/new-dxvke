/* pages/allIncome/allIncome.wxss */
.header{
  height: 218rpx;
  background: #fdc71b;
  padding: 18rpx 54rpx 48rpx;
  box-sizing: border-box;
}
.header .num{
  line-height:110rpx;
  font-size: 80rpx;
  color: white;
  font-weight: bold;
}
.header .title{
  line-height:42rpx;
  color: rgba(255, 255, 255, 0.8);
  font-size: 24rpx;
}
.withdraw{
  background: white;
  height: 104rpx;
  padding: 0 30rpx 0 22rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #333;
}
.withdraw>view{
   display: flex;
  align-items: center;
}
.item-icon{
  width: 42rpx;
  height: 42rpx;
  margin-right: 17rpx;
}
.icon{
  width: 24rpx;
  height: 24rpx;
}
.main{
  background: white;
  margin-top: 20rpx;
}
.list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 140rpx;
  margin-left: 30rpx;
  padding-right: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.list-title{
  line-height: 44rpx;
  font-size: 28rpx;
  color: #333;
}
.list-time{
  font-size: 24rpx;
  color: #999;
}
.list-money{
  font-size: 32rpx;
  color: #333;
}



/*弹出层*/
  .model_bd {
    width: 100%;
    height: 100%;
    opacity: .5;
    background-color: black;
    position: fixed;
    top: 0;
    z-index: 9999;
  }

  .model_main {
    z-index: 99999;
    position: fixed;
    top: 0;
    left: 0;
    width: 598rpx;
    height: 434rpx;
    margin: 346rpx calc((100% - 598rpx) / 2);
    background-color: white;
    border-radius: 15rpx;
  }

  .model_main_des {
    position: absolute;
    text-align: center;
    width: 100%;
  }

  .model_main_des > .des {
    font-size: 20rpx;
    color: #666666;
    margin: 0 16rpx;
    line-height: 40rpx;
    height: 40rpx;
  }

  .m_btn {
    display: inline-block;
    font-size: 28rpx;
    color: white;
    width: 464rpx;
    height: 64rpx;
    line-height: 64rpx;
    border-radius: 80rpx;
    border: none;
    margin: 10rpx 0;
    background: #9a7bff;
  }

  .model_main image {
    width: 76rpx;
    height: 76rpx;
    position: absolute;
    bottom: -130rpx;
    left: calc((100% - 76rpx)/2);
  }

  .model_title {
    color: #333;
    font-size: 32rpx;
    text-align: center;
    height: 114rpx;
    line-height:114rpx;
  }

  .word {
    width: 450rpx;
    -webkit-appearance: none;
    -webkit-tap-highlight-color: rgba(255, 0, 0, 0);
    text-align: center;
    font-size: 22rpx;
    color: #333;
    height: 90rpx;
    text-decoration: underline;
    margin:0 auto;
  }

  .cancel {
    width: 50rpx;
    height: 50rpx;
    position: fixed;
    right: calc(((100% - 320rpx) / 2) - 50rpx);
    top: calc(((100% - 320rpx) / 2) - 300rpx);
    z-index: 999999;
  }