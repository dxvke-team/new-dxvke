/* pages/allIncome/allIncome.wxss */
.header{
  height: 218rpx;
  background: #fdc71b;
  padding: 18rpx 54rpx 48rpx;
  box-sizing: border-box;
}
.header .num{
  line-height:110rpx;
  font-size: 80rpx;
  color: white;
  font-weight: bold;
}
.header .title{
  line-height:42rpx;
  color: rgba(255, 255, 255, 0.8);
  font-size: 24rpx;
}
.withdraw{
  background: white;
  height: 104rpx;
  padding: 0 30rpx 0 22rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #333;
}
.withdraw>view{
   display: flex;
  align-items: center;
}
.item-icon{
  width: 42rpx;
  height: 42rpx;
  margin-right: 17rpx;
}
.icon{
  width: 24rpx;
  height: 24rpx;
}
.main{
  background: white;
  margin-top: 20rpx;
}
.list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 140rpx;
  margin-left: 30rpx;
  padding-right: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.list-title{
  line-height: 44rpx;
  font-size: 28rpx;
  color: #333;
}
.list-time{
  font-size: 24rpx;
  color: #999;
}
.list-money{
  font-size: 32rpx;
  color: #333;
}